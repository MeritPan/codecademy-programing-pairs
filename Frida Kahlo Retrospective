paintings = ["The Two Fridas", "My Dress Hangs Here", "Tree of Hope", "Self Portrait With Monkeys"]
dates = [1939, 1933, 1946, 1940]

paintings = list(zip(paintings, dates))

print(paintings)

paintings.extend(['The Broken Column', 1944])
paintings.extend(['The Wounded Deer', 1946])
paintings.extend(['Me and My Doll', 1937])

print(len(paintings))

audio_tour_number = list(range(1, 11))

master_list = list(zip(audio_tour_number, paintings))

print(master_list)
